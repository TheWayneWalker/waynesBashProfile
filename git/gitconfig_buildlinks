[alias]
    co = checkout
    st = status
    sa = stash
    br = branch
    brs = branch -avv
    lgl = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
    lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    #lgl = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
    pr = pull --rebase
    r = rebase
    rh = reset HEAD
    prblue  = "!f() { git pull https://stash.buildlinks.net/scm/bnod/blue-ui.git              refs/pull-requests/$1/from; } ; f"
    prplan  = "!f() { git pull https://stash.buildlinks.net/scm/bnod/plan-service.git         refs/pull-requests/$1/from; } ; f"
    prnhood = "!f() { git pull https://stash.buildlinks.net/scm/bnod/neighborhood-service.git refs/pull-requests/$1/from; } ; f"
    commitfail = "!f() { git commit -m < curl whatthecommit.com } ; f"
